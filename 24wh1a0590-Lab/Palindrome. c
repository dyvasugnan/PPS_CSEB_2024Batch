#include <stdio.h>
int main() {
    int n,rem,n1,rev=0;
    printf("\n enter n value:");
    scanf("%d",&n);
    n1=n;
    while(n>0)
    {
    rem=n%10;
    rev=rev*10+rem;
    n=n/10;
}
if(n1==rev)
{
    printf("%d is palindrome",n1);
}
else
{
    printf("%d is not a palindrome",n1);
}
    return 0;
}
