#include<stdio.h>

void main()

{

int a[100], i, *ptr,n;

printf("enter the size of the array: ");

scanf("%d",&n);

printf("Enter the values of the array: ");

for(i = 0; i < n; i++)

scanf("%d", &a[i]);

ptr = &a[n - 1];

printf("\nElements of array in reverse order ...\n");

for(i = 0; i < n; i++)

printf("%d\n", *ptr--);

}







